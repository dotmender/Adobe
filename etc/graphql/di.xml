<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\StoreGraphQl\Model\Resolver\Store\StoreConfigDataProvider">
        <arguments>
            <argument name="extendedConfigData">
                <item name="max_try" xsi:type="string">otp_login/otp_config/max_try</item>
                <item name="otp_expired_time" xsi:type="string">otp_login/otp_config/otp_expired_time</item>
                <item name="otp_validation_time" xsi:type="string">otp_login/otp_config/otp_validation_time</item>
                <item name="otp_locking_period" xsi:type="string">otp_login/otp_config/otp_locking_period</item>
                <item name="interval_reminder" xsi:type="string">otp_login/otp_config/interval_reminder</item>
                <item name="resend_otp" xsi:type="string">otp_login/otp_config/resend_otp</item>
                <item name="otp_format" xsi:type="string">otp_login/otp_config/otp_format</item>
                <item name="otp_length" xsi:type="string">otp_login/otp_config/otp_length</item>
            </argument>
        </arguments>
    </type>
</config>
